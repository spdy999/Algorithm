filters:
  and:
    - file.ext == "md"
    - not:
        - file.name.startsWith("@IND_")
        - file.folder.contains("Readwise")
        - file.folder.contains("98-Glasp")
views:
  - type: table
    name: Table
    order:
      - file.name
      - file.mtime
      - file.ctime
      - file.folder
    sort:
      - property: file.mtime
        direction: DESC
    columnSize:
      file.name: 471
